{
  "version": 2,
  "alias": "frontend-testing",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build"
    }
  ],
  "routes": [
    {
      "src": "^/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "index.html",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html/$1"
    }
  ]
}