{
  "version": 2,
  "alias": "the-pragmatic-frontend-tester",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build"
    }
  ],
  "routes": [
    {
      "src": "^/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "index.html",
      "dest": "/index.html"
    }
  ]
}