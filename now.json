{
  "version": 2,
  "alias": "frontend-testing-dotjs",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build"
    }
  ],
  "routes": [
    {
      "src": "^/images/(.*)",
      "dest": "/images/$1"
    },
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ]
}